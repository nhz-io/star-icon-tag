{
  "name": "star-icon-tag",
  "version": "0.0.1",
  "description": "Make SVG Stars",
  "license": "MIT",
  "repository": "nhz-io/star-icon-tag",
  "author": {
    "name": "Ishi Ruy",
    "email": "dev@nhz.io",
    "url": "nhz.io/star-icon-tag"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "xo && ava",
    "babel": "./node_modules/.bin/babel",
    "start": "npm test && npm run babel -- src/star-icon-tag.js -o index.js",
    "prepublish": "npm start"
  },
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "keywords": [
    "nhz.io",
    "riot",
    "riot-tag",
    "star",
    "stars",
    "svg",
    "icon",
    "star-icon"
  ],
  "dependencies": {
    "riot": "^2.3.17"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "jsdom": "^8.1.0",
    "xo": "^0.12.1"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ],
    "ignore": "index.js",
    "rules": {
      "semi": [
        1,
        "never"
      ],
      "comma-dangle": [
        1,
        "always-multiline"
      ]
    }
  },
  "ava": {
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  }
}
